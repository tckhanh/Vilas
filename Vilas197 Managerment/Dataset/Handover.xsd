                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        (SELECT        SUM(1) AS Expr1
                          FROM            HandoverAcc AS HA
                          WHERE        (HandAccID &lt;= HandoverAcc.HandAccID) AND (HandoverID = @HandoverID)) AS No, TestAccessory.AccName, TestAccessory.Model, TestAccessory.SN, 
                         TestAccessory.Manufacturer, TestSysStatus.TestSysStatusName, HandoverAcc.HandoverID, HandoverAcc.HandAccID, TestSysStatus.TestSysStatusID, 
                         TestAccessory.AccNo
FROM            HandoverAcc INNER JOIN
                         TestSysStatus ON HandoverAcc.Status = TestSysStatus.TestSysStatusID INNER JOIN
                         TestAccessory ON HandoverAcc.AccID = TestAccessory.AccID INNER JOIN
                         AccHoldStatus ON TestAccessory.Hold = AccHoldStatus.AccHoldID
WHERE        (HandoverAcc.HandoverID = @HandoverID)
ORDER BY TestSysStatus.TestSysStatusID</CommandText>
