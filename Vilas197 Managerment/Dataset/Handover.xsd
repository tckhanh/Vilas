                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Handover.HandoverID, Handover.HandoverNo, Handover.HandoverDate, Handover.HandoverReceiveEx, Handover.HandoverReceiveRe, Handover.CompanyName, 
                         Handover.Address, Handover.PerName, Handover.PerID, Handover.PerPhoneNo, HandoverStatus.HandStatusName, Staff.StaffFullName AS HandoverStaffName, 
                         Handover.Note, Handover.Position, Staff.ID AS StaffID, Staff.Position AS StaffPosition, Handover.Phone, Handover.Fax
